{
    "extends": "@vue/tsconfig/tsconfig.json",
    "compilerOptions": {
        "paths": {
            "@/*": ["./resources/js/*"],
            "#build/ui": [
                "./node_modules/@nuxt/ui/.nuxt/ui"
            ]
        },
        "types": ["vite/client"]
    },
    "include": [
        "resources/js/**/*.ts",
        "resources/js/**/*.d.ts",
        "resources/js/**/*.tsx",
        "resources/js/**/*.vue",
        "resources/types/**/*.ts",
        "resources/types/**/*.d.ts",
        "auto-imports.d.ts",
        "components.d.ts"
    ]
}
